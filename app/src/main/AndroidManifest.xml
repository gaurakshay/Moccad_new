<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.ou.cs.moccad_new">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".QueryResults"
            android:label="@string/title_activity_query_results"
            android:theme="@style/AppTheme.NoActionBar">
            <action android:name="edu.ou.cs.moccad_new.QueryResults" />

            <category android:name="android.intent.category.DEFAULT" />
        </activity>
        <activity
            android:name=".QueryBuilder"
            android:label="Query Builder"
            android:theme="@style/AppTheme.NoActionBar">
            <action android:name="edu.ou.cs.moccad_new.QueryBuilder" />

            <category android:name="android.intent.category.DEFAULT" />
        </activity>

    </application>

</manifest>
